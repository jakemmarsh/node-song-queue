  "author": "Jake Marsh <jakemmarsh@gmail.com> (http://jakemmarsh.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakemmarsh/node-song-queue/issues"
  },
  "homepage": "https://github.com/jakemmarsh/node-song-queue",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "mocha": "^2.3.4",
    "should": "^8.1.1",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  }
