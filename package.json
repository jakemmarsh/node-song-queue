  "description": "A node module for creating and operating on a song queue. Includes history, shuffling, and repetition.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "babel lib/index.js --watch --out-file dist/index.js --source-maps inline",
    "test": "mocha --compilers js:babel-register",
    "test-watch": "mocha --watch --compilers js:babel-register",
    "build": "babel lib/index.js --out-file dist/index.js",
    "prepublish": "npm run-script build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jakemmarsh/node-song-queue.git"
  },
  "keywords": [
    "song",
